{
  "name": "@taildown/linter",
  "version": "0.1.0",
  "description": "Professional linter for Taildown syntax with auto-fix capabilities",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest"
  },
  "keywords": [
    "taildown",
    "linter",
    "syntax",
    "markdown",
    "validation"
  ],
  "author": "Taildown Team",
  "license": "MIT",
  "dependencies": {
    "@taildown/compiler": "workspace:*",
    "@taildown/shared": "workspace:*",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}

